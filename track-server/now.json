{
    
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "index.js"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb-uri"
    }
    
}
